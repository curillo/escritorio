{
  "$schema": "http://json.schemastore.org/tsconfig.json",
  "compilerOptions": {
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "es6",
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noFallthroughCasesInSwitch": true,
    "noUnusedLocals": true,
    "strictNullChecks": true,
    "sourceMap": false,
    "esModuleInterop": true
  },
  "compileOnSave": true
}
